<div class="contain-to-grid sticky shadow-panel nav-bar-color highlights">
  <nav class="top-bar nav-bar-color">
    <ul class="title-area">
      <!-- Title Area -->
      <li class="name">
        <a href="/"><%= image_tag "logo.png", {class: "logo-sml"} %></a>
      </li>
      <!-- Remove the class "menu-icon" to get rid of menu icon. Take out "Menu" to just have icon alone -->
      <li class="toggle-topbar menu-icon"><a href="#"><span>Menu</span></a></li>
    </ul>

    <section class="top-bar-section">
      <!-- Left Nav Section -->
      <% if session[:developer_id] == nil %>
        <ul class="left">
          <% @projects.each_with_index do |project, i| %>
            <li class="divider"></li>
            <li><a href="#<%= i %>">Project: <%= project.title %></a></li>
          <% end %>
        </ul>
      <% end %>
      <!-- Right Nav Section -->
      <% if session[:developer_id] != nil %>
        <ul class="right">
          <li><%= link_to "Logout", logout_path %></li>
        </ul>
      <% end %>
    </section>
  </nav>
</div>