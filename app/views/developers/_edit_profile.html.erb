<div class="panel">
  <div class="row">
    <%= render 'projects/form', project: @new_project %>
  </div>

  <div class="row">
    <div class="large-12 columns">
      <h4>My Projects</h4>
      <div class="row collapse">

        <% @projects.each do |project| %>
          <%= render 'projects/form', project: project %>
        <% end %>

        <%= render partial: 'repositories' %>
      </div>
    </div>
  </div>
</div>


